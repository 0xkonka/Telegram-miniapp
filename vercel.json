{
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/scripts/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/src/scripts/(.*)",
      "dest": "src/scripts/$1"
    }
  ]
}
